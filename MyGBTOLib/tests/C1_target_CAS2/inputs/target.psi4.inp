# HF
set reference       rhf
set maxiter         100

# CASSCF
set restricted_docc [10]
set active          [ 4]
set mcscf_maxiter   1000

# state averaging
set num_roots       4
set avg_states      [ 0, 1, 2, 3 ]
set avg_weights     [ 1, 1, 1, 1 ]

[E, wfn] = energy('casscf', return_wfn=True)

molden(wfn, 'nitrous.acid.molden')
