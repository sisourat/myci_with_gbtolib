# HF
set reference       rhf
set soscf           True  # second-order SCF to avoid convergence issues
set soscf_max_iter  25
set soscf_start_convergence 1e-3

# CASSCF
set restricted_docc [10,8]
set active          [ 5,3]
set mcscf_maxiter   1000

# state averaging
set num_roots       6
set avg_states      [ 0, 1, 2, 3, 4, 5 ]
set avg_weights     [ 1, 1, 1, 1, 1, 1 ]

[E, wfn] = energy('casscf', return_wfn=True)

molden(wfn, 'butanen2.molden')
